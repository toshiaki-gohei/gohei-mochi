    "eventemitter3": "3.0.x",
    "jconv": "0.1.x",
    "js-cookie": "2.2.x",
    "normalize.css": "7.0.x",
    "preact": "8.2.x",
    "redux": "3.7.x"
  },
  "devDependencies": {
